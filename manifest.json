{
  "manifest_version": 2,

  "name": "Simple Password Generator",
  "homepage_url": "https://github.com/araemot/password-generator",

  "description": "Generates a random password using the browser's crypto RNG.",
  "version": "2018.5.28",

  "icons": {
    "48": "dice48.png",
    "96": "dice96.png"
  },

  "browser_action": {
    "browser_style": false,
    "default_icon": "dice.png",
    "default_popup": "popup.html"
  }
}
